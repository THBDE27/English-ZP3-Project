@model List<Technology>
@{
    ViewData["Title"] = "Technology";
}

<link rel="stylesheet" href="~/css/timeline.css" asp-append-version="true" />

<h1 class="title">Technology</h1>


<span> timeline of technology related to swimming</span>


<div class="center">
    <div class="slider">
        <div class="cards">
