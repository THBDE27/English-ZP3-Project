@model List<Technology>
@{
    ViewData["Title"] = "Technology";
}

<link rel="stylesheet" href="~/css/timeline.css" asp-append-version="true" />


<h1 class="title">Technology</h1>


<div class="box">

    <div class="timeline">
        <ul>
             @foreach (Technology tech in Model)
             {

                <li>
                    <div class="technology">

                   
                        <h3 class="date">
                            @tech.Date
                        </h3>

                        <h2>
                            @tech.Name
                        </h2>

                        <p>
                            @tech.Info
                        </p>


                    </div>

                </li>
            }

        </ul>
    </div>
</div>